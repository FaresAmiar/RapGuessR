<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rapguessr">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FinActivity"
            android:label="@string/title_activity_fin"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".JeuDifficileActivity"
            android:label="@string/title_activity_jeu_difficile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".JeuFacileActivity"
            android:label="@string/title_activity_jeu_facile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChoixRapperActivity"
            android:label="@string/title_activity_choix_rapper"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>